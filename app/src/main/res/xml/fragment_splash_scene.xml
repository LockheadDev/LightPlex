<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/end"
        motion:autoTransition="animateToEnd"
        motion:duration="2750"
        motion:motionInterpolator="cubic(0.21,0.88,0.48,1)">
        <KeyFrameSet >
            <KeyAttribute
                motion:motionTarget="@+id/splashImageView"
                motion:framePosition="0"
                android:alpha="0.0" />
            <KeyAttribute
                motion:motionTarget="@+id/circleImageView"
                motion:framePosition="0"
                android:scaleX="0.1" />
            <KeyAttribute
                motion:motionTarget="@+id/circleImageView"
                motion:framePosition="0"
                android:scaleY="0.1" />
            <KeyAttribute
                android:scaleX="0.1"
                motion:motionTarget="@+id/circleImageView"
                motion:framePosition="50" />
            <KeyAttribute
                android:scaleY="0.1"
                motion:motionTarget="@+id/circleImageView"
                motion:framePosition="50" />
            <KeyAttribute
                motion:motionTarget="@+id/splashImageView"
                motion:framePosition="0"
                android:rotation="60" />
            <KeyAttribute
                motion:motionTarget="@+id/splashImageView"
                motion:framePosition="50"
                android:rotation="0" />
            <KeyAttribute
                motion:motionTarget="@+id/splashImageView"
                motion:framePosition="50"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circleImageView"
                motion:framePosition="0"
                android:alpha="0.0" />
            <KeyAttribute
                motion:motionTarget="@+id/circleImageView"
                motion:framePosition="50"
                android:alpha="0.0" />
            <KeyAttribute
                android:scaleX="15"
                motion:motionTarget="@+id/circleImageView"
                motion:framePosition="100" />
            <KeyAttribute
                android:scaleY="15"
                motion:motionTarget="@+id/circleImageView"
                motion:framePosition="100" />
        </KeyFrameSet>
    </Transition>
</MotionScene>